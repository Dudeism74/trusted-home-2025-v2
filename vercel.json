{
  "version": 2,
  "builds": [
    {
      "src": "studio/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "src": "/studio/(.*)", "dest": "/studio/$1" },
    { "src": "/admin/(.*)", "dest": "/studio/$1" }
  ]
}
